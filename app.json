{
  "expo": {
    "name": "SmashIt_Client",
    "scheme": "smashit",
    "slug": "SmashIt_Client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logoCourt.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoCourt.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/logoCourt.png"
    },
    "plugins": ["expo-font", "expo-router", "expo-secure-store"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e9a313d7-80e7-4452-9cb0-fe7d4a669a7e"
      }
    }
  }
}
